<Window x:Class="Vista___OnBreak.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Vista___OnBreak"
        mc:Ignorable="d"
        Title="Menú" Height="450" Width="800">
    <Grid>
        <Menu HorizontalAlignment="Left" Height="39" VerticalAlignment="Top" Width="792">
            <MenuItem Header="Opciones" Height="39" Width="132" FontSize="18">
                <MenuItem x:Name="AdmCli" Header="Administracion de Clientes" Height="39" Margin="0,0,-1,0" Click="Registro_Click" />
                <MenuItem x:Name="LisCli" Header="Listado de Clientes" Click="MenuItem_Click" Margin="0,0,3,0"/>
                <MenuItem x:Name="AdmCon" Header="Administración de Contratos" Margin="0,0,3,0"/>
                <MenuItem x:Name="LisCon" Header="Listado de Contratos" HorizontalAlignment="Left" Width="321" Margin="0,0,-1,0"/>
                <MenuItem x:Name="Salir" Header="Salir" HorizontalAlignment="Left" Width="320"/>
            </MenuItem>
        </Menu>



    </Grid>
</Window>
